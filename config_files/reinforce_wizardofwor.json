{
  "reinforce_wizardOfWor": {
    "agent": [{
      "name": "REINFORCE_wow",
      "algorithm": {
        "name": "Reinforce",
        "action_pdtype": "Categorical",
        "action_policy": "epsilon_greedy",
        "center_return": true,
        "explore_var_spec": {
          "name": "linear_decay",
          "start_value": 1.0,
          "end_val": 0.01,
          "start_step": 0,
          "end_step": 50000
        },
        "gamma": 0.9,
        "entropy_coef_spec": {
          "name": "linear_decay",
          "start_val": 0.01,
          "end_val": 0.001,
          "start_step": 0,
          "end_step": 20000
        },
        "training_frequency": 1
      },
      "memory": {
        "name": "OnPolicyReplay"
      },
      "net": {
        "type": "ConvNet",
        "shared": true,
        "conv_hid_layers": [
          [32, 7, 4, 0, 1],
          [64, 5, 2, 0, 1],
          [32, 5, 1, 0, 1]
        ],
        "fc_hid_layers": [128],
        "hid_layers_activation": "relu", 
        "out_layer_activation": "tanh",
        "init_fn": null,
        "normalize": true,
        "batch_norm": false,
        "clip_grad_val": 1.0,
        "loss_spec":{
          "name": "MSELoss"
        },
        "optim_spec":{
          "name": "Adam",
          "lr": 0.03
        },
        "lr_scheduler_spec": {
          "name": "StepLR",
          "step_size": 30,
          "gamma": 0.1
        },
        "update_type": "replace",
        "update_frequency": 1000,
        "gpu": true
      }
    }],
    "env": [{
      "name": "WizardOfWor-v0",
      "max_t": null,
      "max_frame": 100000,
      "frame_op": "concat",
      "frame_op_len": 4,
      "reward_scale": "sign",
      "num_envs": 16
    }],
    "body": {
      "product": "outer",
      "num": 1
    },
    "meta": {
      "distributed": false,
      "eval_frequency": 5000,
      "max_session": 4,
      "max_trial": 10
    },
    "search": {
      "agent": [{
        "net":{
          "fc_hid_layers__choice": [128, 64, 32],
          "batch_norm__choice": [true, false],
          "lr__uniform": [0.001, 0.05]
        }
      }]
    }
  }
}
